{
  "name": "cjtest",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx ts-node build.ts",
    "start": "node -r module-alias/register ./dist",
    "dev": "nodemon"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@src": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avas27027/cjtest.git"
  },
  "author": "Alvaro Sotelo Vila",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/avas27027/cjtest/issues"
  },
  "homepage": "https://github.com/avas27027/cjtest#readme",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "firebase-admin": "^11.11.1",
    "fs-extra": "^11.1.1",
    "helmet": "^7.1.0",
    "jet-logger": "^1.3.1",
    "jet-validator": "^1.1.1",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "ts-command-line-args": "^2.5.1",
    "tsc-alias": "^1.8.8"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/morgan": "^1.9.9",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  }
}
